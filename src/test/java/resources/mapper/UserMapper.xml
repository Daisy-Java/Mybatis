<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.apache.ibatis.UserMapper">
    <select id="queryList" parameterType="map" resultType="org.apache.ibatis.User">
        select id, nick_name as nickName from zhw_user where id = 1
    </select>
</mapper>
